{
    "name": "shopdev",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon",
        "build": "rimraf ./dist && tsc && tsc-alias",
        "start": "node dist/index.js",
        "start:prod": "NODE_ENV=production node dist/index.js",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:unit": "jest tests/unit",
        "test:integration": "jest tests/integration",
        "test:security": "jest tests/security",
        "test:performance": "jest tests/performance",
        "test:all": "npm run test:unit && npm run test:integration && npm run test:security && npm run test:performance",
        "test:ci": "jest --ci --coverage --watchAll=false",
        "seed": "ts-node src/scripts/seed.ts",
        "migrate": "ts-node src/scripts/migrate.ts",
        "benchmark": "ts-node src/scripts/benchmark.ts",
        "performance:test": "npm run benchmark",
        "dev:optimized": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
        "prod:optimized": "NODE_ENV=production node dist/index.js",
        "cache:clear": "ts-node -e \"import('./src/config/redis').then(m => m.redisCache.flush())\"",
        "db:indexes": "ts-node -e \"import('./src/config/indexes').then(m => m.createOptimizedIndexes())\"",
        "health:check": "curl -s http://localhost:8081/api/v1/performance/health",
        "start:fullstack": "node start-fullstack.js",
        "dev:fullstack": "node start-fullstack.js",
        "metrics": "curl -s http://localhost:8081/api/v1/performance/metrics",
        "test:redis": "ts-node src/scripts/test-redis.ts",
        "cleanup:indexes": "ts-node src/scripts/cleanup-indexes.ts",
        "seed:translations": "ts-node src/scripts/seed-translations.ts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
