        "cors": "^2.8.5",
        "dataloader": "^2.2.3",
        "dotenv": "^17.2.1",
        "express-rate-limit": "^8.0.1",
        "express-validator": "^7.2.1",
        "fast-json-stringify": "^6.0.1",
        "flatted": "^3.3.3",
